{
  "expo": {
    "name": "TestingExamples",
    "slug": "TestingExamples",
    "version": "1.0.0",
    "orientation": "portrait",
    "scheme": "myapp",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "backgroundColor": "#ffffff"
      }
    },
    "plugins": [
      [
        "expo-router",
        {
          "root": "./examples"
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true
    }
  }
}
